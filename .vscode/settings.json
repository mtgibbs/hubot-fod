// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.gitattributes": true,        
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map": true,
        "**/.hubot_history": true,
        "**/npm-debug.log": true
    }
}